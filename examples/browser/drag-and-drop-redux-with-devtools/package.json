  },
  "browser": {
    "vm": "vm-browserify2"
  },
  "main": "src/index.jsx",
  "scripts": {
    "build": "node ../../../bin/cli.js  -i ./src/reducers/drag-and-drop.scxml --compile -o dist/drag-and-drop.js && (cd dist; browserify -d -t babelify ../src/index.js -o ./index.js; sorcery -i index.js)",
